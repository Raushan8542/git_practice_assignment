// Check Prime Number...

let num=13;
let product=0;

for(i=0; i<=num; i++){
if(num%i==0){
product++;
}
}

if(product==2){
console.log("is prime");
}
else{
console.log("is not prime");
}




// Check Palindrome...

let str="eye";
let bag="";
let bag1="";


for(i=0; i<=str.length-1; i++){
bag+=str[i];
}

for(i=str.length-1; i<=0; i++){
bag1+=str[i];
}

bag==bag1 ? console.log(str+"is palindrome"); : console.log(str+"is not palindrome");